{
  "name": "figmail",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/handlebars": "^4.0.39",
    "@types/node-fetch": "^2.1.2",
    "handlebars": "^4.0.12",
    "netlify-lambda": "^1.0.2",
    "node-fetch": "^2.2.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.0.5",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:server": "npm-run-all build:server-typescript build:server-lambda",
    "build:server-lambda": "netlify-lambda build ./functions/.build",
    "build:server-typescript": "tsc -p functions --outDir functions/.build",
    "start:server": "npm-run-all build:server-typescript start:server-watch",
    "start:server-watch": "npm-run-all --parallel start:server-typescript start:server-lambda",
    "start:server-lambda": "netlify-lambda serve functions/.build",
    "start:server-typescript": "tsc -w -p functions --outDir functions/.build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
